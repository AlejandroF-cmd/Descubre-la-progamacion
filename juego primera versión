void puntos(){
  text (pts, 10, 10 );
  print("x es");
  println (x);
  print("y es");
  println (y);
  println (pts);
  if (x > 260 && x<320){
    if (y>100 && y<120){
      
      pts = pts + 1;
    }
  }
}
