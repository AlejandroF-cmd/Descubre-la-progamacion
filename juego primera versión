			if (mouseButton==RIGHTBUTTON){
  	pts=0;
    x = 130;
    y =130;
  	animate(dibuja);
      }}}
	if (x>0 && x<20){
		if (y>00 && y<50){
      x = 15;
      y = 45;
        image("https://www.emp-online.es/dw/image/v2/BBQV_PRD/on/demandware.static/-/Sites-master-emp/default/dw1e4465b8/images/1/6/2/8/162852d-emp.jpg?sfrm=png",0,0,320,320);
			if (mouseButton==RIGHTBUTTON){
