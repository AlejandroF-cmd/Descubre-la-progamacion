int x= 130;
int y= 130;
int pts= 0;
int won = 0;
String nombre = readString("Escribe tu nombre");
  int anchoHola = textWidth(" ");
  int anchoEspacio = textWidth(" ");
void main (){
  animate(dibuja);
  
}
void dibuja(){
  image("data:image/jpeg;base64,",0,0,320,320);
  stroke(255,145,0,100);
  fill(255,145,57);
  rect(260,40,50,20);
  rect(260,80,50,20);
  rect(260,120,50,20);
  stroke(255,255,255);
  fill(255,255,255);
  textSize(7);
  text(nombre,10,290);
  text("veces ganado " + won,10,305);
  
  //boton play coords
  if (mouseX >260 && mouseX < 310){
  	if (mouseY >40 && mouseY < 60){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,40,50,20);
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (jogo);
          }}
      
  }}
  //boton ajustes coords
  if (mouseX >260 && mouseX < 310){
  	if (mouseY >80 && mouseY < 100){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,80,50,20);  
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (ajustes);
          }}
        
  }}
  //boton salir
   if (mouseX >260 && mouseX < 310){
  	if (mouseY >120 && mouseY < 140){
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,120,50,20);
      	if(mousePressed){
          if (mouseButton==LEFTBUTTON){
            animate(salir);
          }}
  }}
  

  stroke(255,255,255);
  fill(255,255,255);
  textSize(8);
  text("JUGAR",265,55);
  text("AJUSTES",262,95);
  text("SALIR",265,135);

  
}
void ajustes(){
  image("https://i.pinimg.com/originals/9a/15/9d/9a159d1a669c114728789475d04e02aa.jpg",0,0,320,320);
  stroke(255,145,0,100);
  fill(255,145,57);
  rect(20,40,50,20);
  rect(20,80,50,20);
  stroke(255,255,255);
  fill(255,255,255);
  textSize(7);
  text("AJUSTES",25,55);
  text("CREDITOS",22,95);
  text(nombre,10,290);
  // CONTROLES
  if (mouseX >20 && mouseX < 80){
  	if (mouseY >40 && mouseY < 60){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(20,40,50,20);
      stroke(255,255,255);
  		fill(255,255,255);
  		textSize(7);
  		text("AJUSTES",25,55);
  		text("CREDITOS",22,95);
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (ajustes2);
          }}}
	if (mouseX >20 && mouseX < 80){
  	if (mouseY >80 && mouseY < 100){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(20,80,50,20);
      stroke(255,255,255);
  		fill(255,255,255);
  		textSize(7);
  		text("AJUSTES",25,55);
  		text("CREDITOS",22,95);
    	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (creditos);
          }}}}
  
  if (mouseButton==RIGHTBUTTON){
    animate (dibuja);
  }}}
	
void salir(){
  background(0,0,0);
  if (mouseButton==RIGHTBUTTON){
  animate (dibuja);
  }}
void controles(){
  if (y<0){
    y = 130;
    x = 130;
  }
   if (y>271){
    y = 130;
    x = 130;
  }
   if (x<0){
    x = 130;
    y = 130;
  }
  if (x>271){
    x = 130;
    y = 130;
  }
  if (keyPressed && key=="up"){
    println("arriba");
    y = y -2;
  }
  if (keyPressed && key=="down"){
    println("abajo");
    y = y +2;
  }
   if (keyPressed && key=="right"){
    println("derecha");
    x = x +2;
  }
  if (keyPressed && key=="left"){
    println("izquierda");
    x = x -2;
  }}
void player (){
  image ("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSW0q1xK7ihQaXh--N4j-x7BZMR3Vx3nMs2hw&s",0,0,320,320);
  image("http://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTr4Gx5IwRzZvlA6HJKhsCgeFX2DQJNYVtA8A&s",x,y,50,50);
	// perma caida --> y = y + 2;
}
void jogo(){
  player();
	controles();
  puntos();
  perder();
  ganar();
  if (mouseButton==RIGHTBUTTON){
   animate(dibuja);
  }}

boolean unovez= false;
void ganar(){
	if (pts > 400 && unovez == false){
  	won = won + 1;
    unovez= true;
    animate(win);
  }}
    
    
void win(){     
	image("https://hoosierlottery.com/getmedia/10d36e07-78ea-48d2-8352-122c337e0a8c/Winner-Winner-Chicken-Dinner_280x183.png?width=561&height=367&ext=.png",0,0,320,320); 
  if (mouseButton==RIGHTBUTTON){
  	pts=0;
    x = 130;
    y =130;
      unovez = false;
  	animate(dibuja);
  }
  }

