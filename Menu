int x= 130;
int y= 130;
String nombre = readString("Escribe tu nombre");
  int anchoHola = textWidth(" ");
  int anchoEspacio = textWidth(" ");
void main (){
  animate(dibuja);
  
}
void dibuja(){
  image("data:image/jpeg;base64,",0,0,320,320);
  stroke(255,145,0,100);
  fill(255,145,57);
  rect(260,40,50,20);
  rect(260,80,50,20);
  rect(260,120,50,20);
  stroke(255,255,255);
  fill(255,255,255);
  textSize(7);
  text(nombre,10,290);
  
  //boton nombre por hacer
	
  //boton play coords
  if (mouseX >260 && mouseX < 310){
  	if (mouseY >40 && mouseY < 60){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,40,50,20);
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (oki);
          }}
      
  }}
  //boton ajustes coords
  if (mouseX >260 && mouseX < 310){
  	if (mouseY >80 && mouseY < 100){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,80,50,20);  
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (ajustes);
          }}
        
  }}
  //boton salir
   if (mouseX >260 && mouseX < 310){
  	if (mouseY >120 && mouseY < 140){
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(260,120,50,20);
      	if(mousePressed){
          if (mouseButton==LEFTBUTTON){
            animate(salir);
          }}
  }}
  

  stroke(255,255,255);
  fill(255,255,255);
  textSize(8);
  text("JUGAR",265,55);
  text("AJUSTES",262,95);
  text("SALIR",265,135);

  
}

void otra(){
background(40,40,40);
  text("hola",10,10);
  if (mouseButton==RIGHTBUTTON){
animate (dibuja);
    
  }
}
void BTNIZQ(){
  if(mousePressed){
    if (mouseButton==LEFTBUTTON){
      ellipse(mouseX,mouseY,10,10);
      animate(otra);
    }
  }
}
void BTNDRCH(){
  if(mousePressed){
    if (mouseButton==RIGHTBUTTON){
      ellipse(mouseX,mouseY,10,10);
      animate(otra);
    }
  }
}
void ajustes(){
  image("https://i.pinimg.com/originals/9a/15/9d/9a159d1a669c114728789475d04e02aa.jpg",0,0,320,320);
  stroke(255,145,0,100);
  fill(255,145,57);
  rect(20,40,50,20);
  rect(20,80,50,20);
  stroke(255,255,255);
  fill(255,255,255);
  textSize(7);
  text("AJUSTES",25,55);
  text("CREDITOS",22,95);
  text(nombre,10,290);
  // CONTROLES
  if (mouseX >20 && mouseX < 80){
  	if (mouseY >40 && mouseY < 60){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(20,40,50,20);
      stroke(255,255,255);
  		fill(255,255,255);
  		textSize(7);
  		text("AJUSTES",25,55);
  		text("CREDITOS",22,95);
      	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (ajustes2);
          }}}
	if (mouseX >20 && mouseX < 80){
  	if (mouseY >80 && mouseY < 100){ 
      stroke(255,145,0,100);
			fill(70,37,0);
  		rect(20,80,50,20);
      stroke(255,255,255);
  		fill(255,255,255);
  		textSize(7);
  		text("AJUSTES",25,55);
  		text("CREDITOS",22,95);
    	if(mousePressed){
    			if (mouseButton==LEFTBUTTON){
            animate (creditos);
          }}}}
  
  if (mouseButton==RIGHTBUTTON){
    animate (dibuja);
  }}}
	
void salir(){
  background(0,0,0);
  if (mouseButton==RIGHTBUTTON){
  animate (dibuja);
  }}
void controles(){
  if (y<0){
    y = 130;
  }
   if (y>320){
    y = 130;
  }
   if (x<0){
    x = 130;
  }
  if (x>320){
    x = 130;
  }
   if (y==0){
    y = 130;
  }
  if (keyPressed && key=="up"){
    println("arriba");
    y = y -2;
  }
  if (keyPressed && key=="down"){
    println("abajo");
    y = y +2;
  }
   if (keyPressed && key=="right"){
    println("derecha");
    x = x +2;
  }
  if (keyPressed && key=="left"){
    println("izquierda");
    x = x -2;
  }}
void player (){
  image ("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSW0q1xK7ihQaXh--N4j-x7BZMR3Vx3nMs2hw&s",0,0,320,320);
  image("http://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTr4Gx5IwRzZvlA6HJKhsCgeFX2DQJNYVtA8A&s",x,y,50,50);
}
void oki(){
  player();
	controles();
  if (mouseButton==RIGHTBUTTON){
   animate(dibuja);
  }
  
}
void ajustes2(){
	image("https://i.pinimg.com/736x/e4/10/81/e41081a323f8b3565724a92e595b6c10.jpg",0,0,320,320); 
	text("Arriba=Flecha arriba",200,250);
	text("Abajo=Flecha abajo",200,260);
	text("Izquierda=Flecha izq",200,270);
	text("Derecha=Flecha drch",200,280);
  text("Botón derecho=Menú",200,290);
  text(nombre,10,290);
  if (mouseButton==RIGHTBUTTON){
	animate(ajustes);
  }
}
void creditos(){
  image("https://i.redd.it/xmgapyo6zyt21.png",0,0,320,320);  
  text("HECHO POR:",200,55);
  text("ALEJANDRO FF",200,65);
  
